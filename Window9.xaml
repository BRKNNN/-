<Window x:Class="авторизация_и_регистрация_буркин.Window9"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:авторизация_и_регистрация_буркин"
        mc:Ignorable="d"
        Title="Window9" Height="450" Width="800">
    <Grid>
        <Button Content="Каталог" Margin="550,10,50,390" Click="Button_Click"/>
        <StackPanel Margin="50,0,50,0">
            <TextBlock Text="Личный кабинет" HorizontalAlignment="Center" FontSize="30" FontWeight="Bold" Margin="0 0 0 20"/>
            

            <Grid Margin="0 0 0 20">
                <Button HorizontalAlignment="Left" Content="Редактировать личные данные" Click="Button_Edit_Click" Margin="0,0,0,-30" Width="200"/>
                <Button HorizontalAlignment="Right" Content="Удаление уч. записи" Click="Button_Delite_Click" Margin="0,0,0,-30" Width="200"/>
                
            </Grid>
        </StackPanel>

    </Grid>
</Window>
